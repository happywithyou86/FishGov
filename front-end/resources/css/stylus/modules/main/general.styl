.main {
  .isActive {
    border-color: #1abc9c;
    .search {
      color: #1abc9c;
      transition: all 0.2s ease-in-out
    }
  }
  .total {
    quotes: "“" "”" "‘" "’";
    font-weight: 600;
    font-style: italic;
    color: #16a085;
    &:before {
      color: #16a085;
      content: open-quote;
    }
    &:after {
      color: #16a085;
      content: close-quote;
    }
  }
}
